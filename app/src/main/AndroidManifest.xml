<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="txcap.com.viewdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GIFDemo.GIFActivity" />
        <activity android:name=".FrameAnimDemo.FrameAnimActivity" />
        <activity android:name=".LottileAnim.LottileAnimActivity" />
        <activity android:name=".GIFDemo.GlideGifActivity" />
        <activity android:name=".CustomView.HeadZoomActivity" />
        <activity android:name=".CustomView.PullToZoomActivity" />
        <activity android:name=".LoopManager.LoopManagerActivity" />
        <activity android:name=".LoopManager.GiftPanelActivity" />
        <activity android:name=".CustomView.PullZoomActivity" />
        <activity android:name=".CustomView.ProgressDialogActivity" />
        <activity android:name=".CustomView.UpAnimationActivity" />
        <activity
            android:name=".CustomView.CommonActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".CustomView.TrainingViewActivity"></activity>
    </application>

</manifest>